@model string
@{ Layout = null; }

<!DOCTYPE html>
<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<title>Routing</title>
	<link rel="stylesheet" asp-href-include="lib/twitter-bootstrap/css/*.min.css" />
</head>
<body class="m-1 p-1">
	<h2>GetLegacyURL</h2>
	 The URL requested was: @Model

	<!-- Sample URL -->
	<a asp-route-legacyUrl="/old/.NET_1.0_Class_Library" class="btn btn-sm btn-secondary">This is an outgoing URL</a>
	<p>URL: @Url.Action(null, null, new { legacyUrl = "/old/.NET_1.0_Class_Library" })</p>
</body>
</html>